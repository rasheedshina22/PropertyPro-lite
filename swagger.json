              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/errorResponse1"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": ["Properties"],
        "summary": "Delete a property advert.",
        "security": [
          {
            "Token": []
          }
        ],
        "parameters": [
          {
            "name": "propertyId",
            "in": "path",
            "required": true,
            "description": "property id",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/properties"
                }
              }
            }
          },
          "401": {
            "description": "You are not Authorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/errorResponse1"
