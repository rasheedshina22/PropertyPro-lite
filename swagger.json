    "/property/": {
      "get": {
        "tags": ["Properties"],
        "summary": "Get all property advertisement offering a specific type of property.",
        "parameters": [
          {
            "name": "type",
            "in": "query",
            "required": true,
            "description": "property type. e.g Duplex",
