      }
    },
    "schemas": {
      "properties": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "owner": {
            "type": "integer"
          },
          "status": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "state": {
            "type": "string"
          },
          "purpose": {
            "type": "string"
          },
          "created_on": {
            "type": "string"
          },
          "city": {
            "type": "string"
          },
          "address": {
            "type": "string"
          },
          "price": {
            "type": "float"
          },
          "image_url": {
            "type": "string"
          },         
          "Id": {
